{
  "cli": {
  "version": ">= 12.0.0",
  "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      },
      "channel": "production"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": true,
      "channel": "preview"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    }
  },
  "submit": {
    "production": {}
  }
}
